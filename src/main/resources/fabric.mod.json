{
	"schemaVersion": 1,
	"id": "mikkas-mod",
	"version": "${version}",
	"name": "Mikka's Mod",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/mikkas-mod/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "com.mikka.mod.MikkasMod",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "com.mikka.mod.MikkasModClient",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "com.mikka.mod.MikkasModDataGenerator",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"mikkas-mod.mixins.json",
		{
			"config": "mikkas-mod.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.1",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}